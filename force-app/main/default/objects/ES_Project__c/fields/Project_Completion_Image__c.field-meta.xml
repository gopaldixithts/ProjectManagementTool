<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_Completion_Image__c</fullName>
    <externalId>false</externalId>
    <formula>IMAGE(&quot;/resource/ES_Status_Images/ES_Percent_100.jpg&quot;, &quot;Completed&quot;, 10, (Percentage_Completion__c * 100 )/1.3) &amp; 
IMAGE(&quot;/resource/ES_Status_Images/ES_Percent_0.jpg&quot;, &quot;Zero&quot;, 10, (100 - Percentage_Completion__c * 100 )/1.3) &amp; 
&apos; &apos; &amp; 
TEXT(ROUND(Percentage_Completion__c * 100,0)) &amp; 
&apos;%&apos;</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Project Completion</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
